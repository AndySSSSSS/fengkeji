<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd"> 
<sqlMap namespace="Message"> 
      <select id="getAllMessage" resultClass="org.fengkeji.weixin.entity.ServMessage"> 
        select  
        meg_id as msgId,
        msg_text as msgTxt,
        msg_type as msgType
        from message
   </select> 
</sqlMap> 